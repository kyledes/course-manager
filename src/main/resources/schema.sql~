drop table coursemanager.student;
create table student (student_id Number(10),firstName Varchar2(50), lastName Varchar2(50), email Varchar2(50), phoneNumber Number(10), CONSTRAINT student_pk PRIMARY KEY (student_id)); 

create sequence studentid_seq start with 1 increment by 1;

drop table coursemanager.address;
create table address (address_id Number(10), street Varchar2(50), city varchar2(50), state varchar2(2), zipcode varchar2);

drop table coursemanager.course;
create table course (course_id Number(10), courseName Varchar2(50), location Varchar2(50), description Varchar2(200), CONSTRAINT course_pk PRIMARY KEY (course_id));


